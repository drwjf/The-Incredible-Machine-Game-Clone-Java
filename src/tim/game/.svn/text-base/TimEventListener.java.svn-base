package tim.game;

import java.awt.*;
import java.awt.event.*;
import java.awt.event.KeyEvent;
import java.lang.*;
import java.util.*;

/**
 * Listener interface for classes that will receive events from TimGame
 * to implement. (tim gui, tim world).
 */
public interface TimEventListener extends EventListener  {

    /**
     * Method that receives the TimEvent whenever fireTimEvent is called from TimGame
     * @param event The object holding relevant information about whatever
	 * fired off the event.
     */
    public void timEventReceived(TimEvent event);

}
